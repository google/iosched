    <item quantity="one">
      <xliff:g id="number_of_sessions">%1$d</xliff:g> sesión disponible</item>
    <item quantity="other">
      <xliff:g id="number_of_sessions">%1$d</xliff:g> sesiones disponibles</item>
  </plurals>

    <!-- Shows as a notification on the system bar to alert user that some of their sessions are about to begin. -->
    <plurals name="session_notification_text">
    <item quantity="one">y otra dentro de <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
    <item quantity="other">y otras <xliff:g id="num_sessions">%2$d</xliff:g> dentro de <xliff:g id="remaining_time">%1$d</xliff:g> min.</item>
  </plurals>

