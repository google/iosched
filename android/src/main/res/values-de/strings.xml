    <item quantity="one">
      <xliff:g id="number_of_sessions">%1$d</xliff:g> verfügbare Sitzung</item>
    <item quantity="other">
      <xliff:g id="number_of_sessions">%1$d</xliff:g> verfügbare Sitzungen</item>
  </plurals>

    <!-- Shows as a notification on the system bar to alert user that some of their sessions are about to begin. -->
